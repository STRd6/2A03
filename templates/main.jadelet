- Actions = require "./actions"
- Selector = require "./selector"
- Seek = require "./seek"

.player
  .display
    .title= @title
    .time= @time
    .viz= @canvas
    input.volume(type="range" value=@volume max=100 step=1)
    = Seek @seek
    = Actions(this)
  .playlist= Selector(@songSelect)
  = Selector(@fontSelect)
