- Actions = require "./actions"
- Selector = require "./selector"
- Seek = require "./seek"

.player
  .display
    label
      = "Song"
      = Selector(@songSelect)
    label
      = "SoundFont"
      = Selector(@fontSelect)
    .time= @time
    .viz= @canvas
    input.volume(type="range" value=@volume max=100 step=1)
    = Seek @seek
    = Actions(this)
